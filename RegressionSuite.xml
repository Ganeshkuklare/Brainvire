<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Regression Suite">
    <listeners>
        <listener class-name="com.qa.utility.TestListener" />
    </listeners>
    <test name="Login, Dashboard, Admin, and Employee Tests">
        <parameter name="browser" value="chrome" />
        <parameter name="url" value="https://opensource-demo.orangehrmlive.com/web"/>
<classes>
	
    

    <class name="com.qa.TestLayer.AdminPageTest" />
    <class name="com.qa.TestLayer.EmployeePageTest" />
    
    <class name="com.qa.TestLayer.EmployeePageTest">
                <methods>
                    <exclude name="addEmployeeTest" />
                </methods>
            </class>
</classes>
</test>
</suite>
